<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" fx:id="root" prefHeight="400.0" prefWidth="700.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="Project1.controller.TeacherController">
   <children>
      <Button layoutX="301.0" layoutY="14.0" mnemonicParsing="false" onAction="#onClick" text="Display Student Data" />
      <TableView fx:id="table" layoutX="64.0" layoutY="50.0" prefHeight="300.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="col_roll" minWidth="100.0" prefWidth="200.0" text="Roll No" />
          <TableColumn fx:id="col_name" minWidth="100.0" prefWidth="200.0" text="Name" />
            <TableColumn fx:id="col_marks" minWidth="100.0" prefWidth="200.0" text="Marks" />
        </columns>
      </TableView>
      <Button fx:id="logout" layoutX="339.0" layoutY="361.0" mnemonicParsing="false" onAction="#onLogout" text="logout" />
   </children>
</AnchorPane>
