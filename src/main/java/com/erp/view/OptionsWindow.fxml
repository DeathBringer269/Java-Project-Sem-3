<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="200.0" maxWidth="400.0" minHeight="200.0" minWidth="400.0" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab closable="false" text="Account">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="325.0" layoutY="132.0" mnemonicParsing="false" onAction="#onSignOut" text="Sign out" />
                        <Button layoutX="297.0" layoutY="34.0" mnemonicParsing="false" onAction="#onViewProfile" text="View Profile" />
                        <CheckBox fx:id="clearSavedData" layoutX="207.0" layoutY="136.0" mnemonicParsing="false" text="Clear Saved Data" />
                        <Button layoutX="24.0" layoutY="132.0" mnemonicParsing="false" onAction="#onBack" text="Back" />
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab closable="false" text="Appearance">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </center>
</BorderPane>
