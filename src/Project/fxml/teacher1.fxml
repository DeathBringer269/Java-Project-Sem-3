<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1000.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="Project.controller.Teacher1Controller">
    <stylesheets>
        <URL value="@/Project/css/teacher1.css" />
    </stylesheets>
   <children>
      <HBox alignment="TOP_CENTER" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="500.0" spacing="50.0">
         <children>
            <MenuButton focusTraversable="false" mnemonicParsing="false" text="View SpreadSheets">
              <items>
                <MenuItem mnemonicParsing="false" text="Student Data" />
                <MenuItem mnemonicParsing="false" text="IAT Marks" />
